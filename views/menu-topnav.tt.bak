[% # menu-topnav.tt %]
[%- items = [
    'techniques',
    'about',
   ]
-%]
[%- links = {
        'techniques' => {
            fork => '/fork',
        },
        'about'      => '/about',
   }
%]
          <nav class="navbar navbar-expand-sm fixed-bottom topnav">
            <ul class="navbar-nav">
[%- FOREACH item IN items %]
  [% IF links.$item.keys.size > 0 %]
              <li class="nav-item dropup">
                <a class="nav-link dropdown-toggle" href="#" id="navbardrop" data-toggle="dropdown">
                  Dropdown link
                </a>
                <div class="dropdown-menu">
    [%- FOREACH subitem IN links.$item.keys -%]
                  <a class="dropdown-item" href="[% links.$item.$subitem %]">[% subitem %]</a>
    [% END %]
                </div>
              </li>
  [% ELSE %]
              <li class="nav-item">
                <a class="nav-link" href="[% links.$item %]">[% item %]</a>
              </li>
  [%- END -%]
[% END -%]
            </ul>
          </nav>
